<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content" android:background="#ffffff" android:gravity="center_vertical|center">
<ImageView android:layout_width="wrap_content" android:layout_height="fill_parent" android:id="@+id/image"></ImageView>
<TextView android:id="@+id/mysched_row_time" android:layout_height="wrap_content" android:textColor="#880000" android:textStyle="bold" android:layout_width="70px" android:layout_toRightOf="@+id/image"></TextView>
<TextView android:paddingLeft="5px" android:id="@+id/mysched_row_route" android:layout_toRightOf="@+id/mysched_row_time" android:layout_width="fill_parent" android:layout_height="wrap_content" android:textColor="#000000"></TextView>
<TextView android:layout_width="fill_parent" android:id="@+id/mysched_row_stop" android:layout_height="17px" android:layout_below="@+id/mysched_row_route" android:textSize="12px" android:textColor="#444444" android:layout_toRightOf="@+id/image"></TextView>
</RelativeLayout>
